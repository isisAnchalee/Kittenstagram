<head>
  <link href='http://fonts.googleapis.com/css?family=Raleway:300' rel='stylesheet' type='text/css'>
</head>

<script>
  $(document).ready(function(){
    $('.email-input').on('click', function(){
      $('.email-input').val("");
    });

    $('.password-input').on('click', function(){
      $('.password-input').val("");
    });

    $('.username-input').on('click', function(){
      $('.username-input').val("");
    });

    $(".box").bgswitcher({
       images: ["http://imageshack.com/a/img661/4435/OXCsAm.jpg", "http://stylonica.com/wp-content/uploads/2014/03/Cute-Cats-cats-33440930-1280-800.jpg", "http://imageshack.com/a/img537/7687/3vOAtt.jpg"],
      effect: "fade", // fade, blind, clip, slide, drop, hide
      interval: 5000, // Interval of switching
      loop: true, // Loop the switching
      shuffle: false, // Shuffle the order of an images
      duration: 5000, // Effect duration
      easing: "swing" // Effect easing
    });
  });

</script>